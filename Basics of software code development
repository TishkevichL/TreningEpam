  	static void firstLines() {
		
		int a, b, c;
	
		Scanner scanFirst = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð°: ");
		a = scanFirst.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ b: ");
		b = scanFirst.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ c: ");
