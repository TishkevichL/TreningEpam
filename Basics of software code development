		z = ((a-3) * (b/2)) + c;
		System.out.println(z);
	}
	
//	â„–1.2  Ð’Ñ‹Ñ‡Ð¸ÑÐ»Ð¸Ñ‚ÑŒ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ð²Ñ‹Ñ€Ð°Ð¶ÐµÐ½Ð¸Ñ Ð¿Ð¾ Ñ„Ð¾Ñ€Ð¼ÑƒÐ»Ðµ 
//      (Ð²ÑÐµ Ð¿ÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð¿Ñ€Ð¸Ð½Ð¸Ð¼Ð°ÑŽÑ‚ Ð´ÐµÐ¹ÑÑ‚Ð²Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ñ‹Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ñ): 
//      (ð‘ + âˆš(ð‘2 + 4ð‘Žð‘) / 2ð‘Ž) âˆ’ ð‘Ž^3 * ð‘ + ð‘^âˆ’2.

	static void secondFormula() {
		
		double a, b, c;
		
		Scanner scanSecond = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð°: ");
		a = scanSecond.nextDouble();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ b: ");
		b = scanSecond.nextDouble();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ c: ");
		c = scanSecond.nextDouble();
		
		double eSqrt, res;
		eSqrt = sqrt(pow(b, 2) + (4*a*c));

		res = (b+eSqrt / 2*a) - (pow(a, 3)*c) + (1/pow(b, 2));
		
		if(a==0) {
			System.out.println("Ð ÐµÑˆÐµÐ½Ð¸Ðµ Ð½ÐµÐ²Ð¾Ð·Ð¼Ð¾Ð¶Ð½Ð¾!");
		}else {
			System.out.println(res);
		}
	}
  
// â„–1.3	  Ð’Ñ‹Ñ‡Ð¸ÑÐ»Ð¸Ñ‚ÑŒ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ð²Ñ‹Ñ€Ð°Ð¶ÐµÐ½Ð¸Ñ Ð¿Ð¾ Ñ„Ð¾Ñ€Ð¼ÑƒÐ»Ðµ 
//      (Ð²ÑÐµ Ð¿ÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ Ð¿Ñ€Ð¸Ð½Ð¸Ð¼Ð°ÑŽÑ‚ Ð´ÐµÐ¹ÑÑ‚Ð²Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ñ‹Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ñ): 
//      (ð‘ ð‘–ð‘› ð‘¥ + ð‘ð‘œð‘  ð‘¦ / ð‘ð‘œð‘  ð‘¥ âˆ’ ð‘ ð‘–ð‘› ð‘¦) âˆ— ð‘¡ð‘” ð‘¥*ð‘¦ 

	static void thirdFormula() {
		
    // Ð³Ñ€Ð°Ð´ÑƒÑÑ‹ 
		double x, y;
		
		Scanner scanThird = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ x: ");
		x = scanThird.nextDouble();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ y: ");
		y = scanThird.nextDouble();
		
		double result = ((sin(x) + cos(y)) / (cos(x) - sin(y))) * tan(x * y);
		
		if(cos(x) - sin(y)==0) {
			System.out.println("Ð ÐµÑˆÐµÐ½Ð¸Ðµ Ð½ÐµÐ²Ð¾Ð·Ð¼Ð¾Ð¶Ð½Ð¾!");
		}else {
			System.out.println(result);
		}
	}
	
//	â„–1.4  Ð”Ð°Ð½Ð¾ Ð´ÐµÐ¹ÑÑ‚Ð²Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾ R Ð²Ð¸Ð´Ð° nnn.ddd (Ñ‚Ñ€Ð¸ Ñ†Ð¸Ñ„Ñ€Ð¾Ð²Ñ‹Ñ… Ñ€Ð°Ð·Ñ€ÑÐ´Ð° Ð² Ð´Ñ€Ð¾Ð±Ð½Ð¾Ð¹ Ð¸ Ñ†ÐµÐ»Ð¾Ð¹ Ñ‡Ð°ÑÑ‚ÑÑ…).
//      ÐŸÐ¾Ð¼ÐµÐ½ÑÑ‚ÑŒ Ð¼ÐµÑÑ‚Ð°Ð¼Ð¸ Ð´Ñ€Ð¾Ð±Ð½ÑƒÑŽ Ð¸ Ñ†ÐµÐ»ÑƒÑŽ Ñ‡Ð°ÑÑ‚Ð¸ Ñ‡Ð¸ÑÐ»Ð° Ð¸ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Ð¿Ð¾Ð»ÑƒÑ‡ÐµÐ½Ð½Ð¾Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ñ‡Ð¸ÑÐ»Ð°. 

	static void fourthReverse() {
		
		double r = 123.456;
		System.out.println("ÐÐ°Ñ‡Ð°Ð»ÑŒÐ½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾: " + r);
		
		double rReverse = (r * 1000) % 1000 + (int) r/1000.0;
		System.out.println("ÐšÐ¾Ð½ÐµÑ‡Ð½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾: " + rReverse);
	}
	
//	â„–1.5  Ð”Ð°Ð½Ð¾ Ð½Ð°Ñ‚ÑƒÑ€Ð°Ð»ÑŒÐ½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾ Ð¢, ÐºÐ¾Ñ‚Ð¾Ñ€Ð¾Ðµ Ð¿Ñ€ÐµÐ´ÑÑ‚Ð°Ð²Ð»ÑÐµÑ‚ Ð´Ð»Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾ÑÑ‚ÑŒ Ð¿Ñ€Ð¾ÑˆÐµÐ´ÑˆÐµÐ³Ð¾ Ð²Ñ€ÐµÐ¼ÐµÐ½Ð¸ Ð² ÑÐµÐºÑƒÐ½Ð´Ð°Ñ…. 
//      Ð’Ñ‹Ð²ÐµÑÑ‚Ð¸ Ð´Ð°Ð½Ð½Ð¾Ðµ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ð´Ð»Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾ÑÑ‚Ð¸ Ð² Ñ‡Ð°ÑÐ°Ñ…, Ð¼Ð¸Ð½ÑƒÑ‚Ð°Ñ… Ð¸ ÑÐµÐºÑƒÐ½Ð´Ð°Ñ… Ð² ÑÐ»ÐµÐ´ÑƒÑŽÑ‰ÐµÐ¹ Ñ„Ð¾Ñ€Ð¼Ðµ: ÐÐÑ‡ ÐœÐœÐ¼Ð¸Ð½ SSc. 
 
	static void timeFifth() {
		
		int time;
		
		Scanner scanFifth = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ time: ");
		time = scanFifth.nextInt();
		
		System.out.println("Ð”Ð»Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾ÑÑ‚ÑŒ Ð¿Ñ€Ð¾ÑˆÐµÐ´ÑˆÐµÐ³Ð¾ Ð²Ñ€ÐµÐ¼ÐµÐ½Ð¸ Ð² ÑÐµÐºÑƒÐ½Ð´Ð°Ñ…: " + time);
		 
		if(time==0) return;
		
		int h = time/3600;
		int m = (time - h * 3600) / 60;
		int s = (h==0) ? time - m * 60:time - (h * 3600 + m * 60);
		
		String hh = Integer.toString(h);
		if(hh.length() == 1) hh = "0" + hh;
		
		String mm = Integer.toString(m);
		if(mm.length() == 1) mm = "0" + mm;
		
		String ss = Integer.toString(s);
		if(ss.length() == 1) ss = "0" + ss;
		
		System.out.printf("%sh%sm%ss", hh, mm, ss);
	}
	
//	â„–1.6    Ð”Ð»Ñ Ð´Ð°Ð½Ð½Ð¾Ð¹ Ð¾Ð±Ð»Ð°ÑÑ‚Ð¸ ÑÐ¾ÑÑ‚Ð°Ð²Ð¸Ñ‚ÑŒ Ð»Ð¸Ð½ÐµÐ¹Ð½ÑƒÑŽ Ð¿Ñ€Ð¾Ð³Ñ€Ð°Ð¼Ð¼Ñƒ, ÐºÐ¾Ñ‚Ð¾Ñ€Ð°Ñ Ð¿ÐµÑ‡Ð°Ñ‚Ð°ÐµÑ‚ true, 
//        ÐµÑÐ»Ð¸ Ñ‚Ð¾Ñ‡ÐºÐ° Ñ ÐºÐ¾Ð¾Ñ€Ð´Ð¸Ð½Ð°Ñ‚Ð°Ð¼Ð¸ (Ñ…, Ñƒ) Ð¿Ñ€Ð¸Ð½Ð°Ð´Ð»ÐµÐ¶Ð¸Ñ‚ Ð·Ð°ÐºÑ€Ð°ÑˆÐµÐ½Ð½Ð¾Ð¹ Ð¾Ð±Ð»Ð°ÑÑ‚Ð¸, Ð¸ false â€” Ð² Ð¿Ñ€Ð¾Ñ‚Ð¸Ð²Ð½Ð¾Ð¼ ÑÐ»ÑƒÑ‡Ð°Ðµ.

	static void sixthCoordinates() {
		
		int x, y;
		Scanner scanIndex = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ x: ");
		x = scanIndex.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ y: ");
		y = scanIndex.nextInt();
		
		boolean locatedInTheArea = locatedInTheArea(x, y);
		
		System.out.println(x + "," + y + "," + locatedInTheArea);
	}
	
	static boolean locatedInTheArea(int x, int y) {
		 
		boolean locatedInTheArea = ((x >= -4 && x <= 4) && (y >=-3 && y <=0)) 
						|| ((x >= -2 && x <= 2) && (y >=0 && y <=4));
			
		return locatedInTheArea;
	}
// 2.1	 Ð”Ð°Ð½Ñ‹ Ð´Ð²Ð° ÑƒÐ³Ð»Ð° Ñ‚Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸ÐºÐ° (Ð² Ð³Ñ€Ð°Ð´ÑƒÑÐ°Ñ…). 
//	 ÐžÐ¿Ñ€ÐµÐ´ÐµÐ»Ð¸Ñ‚ÑŒ, ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÐµÑ‚ Ð»Ð¸ Ñ‚Ð°ÐºÐ¾Ð¹ Ñ‚Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸Ðº, Ð¸ ÐµÑÐ»Ð¸ Ð´Ð°, Ñ‚Ð¾ Ð±ÑƒÐ´ÐµÑ‚ Ð»Ð¸ Ð¾Ð½ Ð¿Ñ€ÑÐ¼Ð¾ÑƒÐ³Ð¾Ð»ÑŒÐ½Ñ‹Ð¼. 
 
	static void firstTriangle() {
		
		int a, b;
		Scanner scanAngle = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð³Ñ€Ð°Ð´ÑƒÑ ÑƒÐ³Ð»Ð° Ð°: ");
		a = scanAngle.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð³Ñ€Ð°Ð´ÑƒÑÑƒÐ³Ð»Ð° b: ");
		b = scanAngle.nextInt();
		
		boolean existTriangle = existTriangle(a, b);
		boolean rectangularTriangle = rectangularTriangle(a, b);
		
		System.out.println("Ð¢Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸Ðº ÑÑƒÑ‰ÐµÑÑ‚Ð²ÑƒÐµÑ‚: "+ existTriangle);
		System.out.println("Ð¢Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸Ðº Ð¿Ñ€ÑÐ¼Ð¾ÑƒÐ³Ð¾Ð»ÑŒÐ½Ñ‹Ð¹: "+ rectangularTriangle);
	}
	
//	CÑƒÑ‰ÐµÑÑ‚Ð²Ð¾Ð²Ð°Ð½Ð¸Ðµ Ñ‚Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸ÐºÐ°
	static boolean existTriangle(int a, int b) {
		boolean anglesExist = a>0 && b>0;
		boolean existTriangle = (a + b > 0 && a + b < 180 && anglesExist);
		return existTriangle;
	}
	
//	Ð¢Ñ€ÐµÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¸Ðº Ð¿Ñ€ÑÐ¼Ð¾ÑƒÐ³Ð¾Ð»ÑŒÐ½Ñ‹Ð¹
	static boolean rectangularTriangle(int a, int b) {
		int c = 180 - (a + b);
		
		boolean rectangularTriangle = (a==90 || b==90 || c==90 );
		
		return rectangularTriangle;
	}
	
//	â„–2.2	ÐÐ°Ð¹Ñ‚Ð¸ max{min(a, b), min(c, d)}. 

	static void secondMax() {
		
		int a, b, c, d;
		
		Scanner scanNum = new Scanner(System.in);

		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð°: ");
		a = scanNum.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ b: ");
		b = scanNum.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ c: ");
		c = scanNum.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ d: ");
		d = scanNum.nextInt();
		
		int max = Math.max(Math.min(a, b), Math.min(c, d));
		
		System.out.println("Max: "+ max);
	}
	
//	â„–2.4	 Ð—Ð°Ð´Ð°Ð½Ñ‹ Ñ€Ð°Ð·Ð¼ÐµÑ€Ñ‹ Ð, Ð’ Ð¿Ñ€ÑÐ¼Ð¾ÑƒÐ³Ð¾Ð»ÑŒÐ½Ð¾Ð³Ð¾ Ð¾Ñ‚Ð²ÐµÑ€ÑÑ‚Ð¸Ñ Ð¸ Ñ€Ð°Ð·Ð¼ÐµÑ€Ñ‹ Ñ…, Ñƒ, z ÐºÐ¸Ñ€Ð¿Ð¸Ñ‡Ð°.
//		 ÐžÐ¿Ñ€ÐµÐ´ÐµÐ»Ð¸Ñ‚ÑŒ, Ð¿Ñ€Ð¾Ð¹Ð´ÐµÑ‚ Ð»Ð¸ ÐºÐ¸Ñ€Ð¿Ð¸Ñ‡ Ñ‡ÐµÑ€ÐµÐ· Ð¾Ñ‚Ð²ÐµÑ€ÑÑ‚Ð¸Ðµ. 

	static void fourthBrick() {
		
		int A, B;
				
		Scanner scanSize = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ A: ");
		A = scanSize.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ B: ");
		B = scanSize.nextInt();
		
		int x, y, z;
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ x: ");
		x = scanSize.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ y: ");
		y = scanSize.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ z: ");
		z = scanSize.nextInt();
		
		boolean res = (x<=A && y<=B) || (x<=B && y<=A) || (z<=A && x<=B) || (z<=B && x<=A);
		System.out.println(res);
	}
	
//	â„–2.5	 Ð’Ñ‹Ñ‡Ð¸ÑÐ»Ð¸Ñ‚ÑŒ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ñ„ÑƒÐ½ÐºÑ†Ð¸Ð¸:
//		F(x) = x^2 - 3x + 9, ÐµÑÐ»Ð¸ x<=3;
//		F(x) = 1 / x^3 + 6, ÐµÑÐ»Ð¸ x>3;

	static void fifthFunctionValue() {
		
		double x;
		
		Scanner scanVal = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ x: ");
		x = scanVal.nextDouble();
		
		double F = x <= 3 ? (pow(x, 2) - 3*x + 9) : (1/(pow(x, 3)+6)); 
		
		System.out.println(F);
	}
	
//	â„–3.1	ÐÐ°Ð¿Ð¸ÑˆÐ¸Ñ‚Ðµ Ð¿Ñ€Ð¾Ð³Ñ€Ð°Ð¼Ð¼Ñƒ, Ð³Ð´Ðµ Ð¿Ð¾Ð»ÑŒÐ·Ð¾Ð²Ð°Ñ‚ÐµÐ»ÑŒ Ð²Ð²Ð¾Ð´Ð¸Ñ‚ Ð»ÑŽÐ±Ð¾Ðµ Ñ†ÐµÐ»Ð¾Ðµ Ð¿Ð¾Ð»Ð¾Ð¶Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾.
//		Ð Ð¿Ñ€Ð¾Ð³Ñ€Ð°Ð¼Ð¼Ð° ÑÑƒÐ¼Ð¼Ð¸Ñ€ÑƒÐµÑ‚ Ð²ÑÐµ Ñ‡Ð¸ÑÐ»Ð° Ð¾Ñ‚ 1 Ð´Ð¾ Ð²Ð²ÐµÐ´ÐµÐ½Ð½Ð¾Ð³Ð¾ Ð¿Ð¾Ð»ÑŒÐ·Ð¾Ð²Ð°Ñ‚ÐµÐ»ÐµÐ¼ Ñ‡Ð¸ÑÐ»Ð°. 
 
	static void summNumbers() {
		
		int numb;
		Scanner scanNumb = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð»ÑŽÐ±Ð¾Ðµ Ñ†ÐµÐ»Ð¾Ðµ Ð¿Ð¾Ð»Ð¾Ð¶Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ð¾Ðµ Ñ‡Ð¸ÑÐ»Ð¾: ");
		numb = scanNumb.nextInt();
		
		int summNumb = 0;
		
		for(int i=1; i<numb; i++) {
			summNumb += i;
		}
		System.out.println("Ð¡ÑƒÐ¼Ð¼Ð° Ñ‡Ð¸ÑÐµÐ» Ð¾Ñ‚ 1 Ð´Ð¾ " + numb + "=" + summNumb);
	}
	
//	â„–3.3	 ÐÐ°Ð¹Ñ‚Ð¸ ÑÑƒÐ¼Ð¼Ñƒ ÐºÐ²Ð°Ð´Ñ€Ð°Ñ‚Ð¾Ð² Ð¿ÐµÑ€Ð²Ñ‹Ñ… ÑÑ‚Ð° Ñ‡Ð¸ÑÐµÐ». 
	static void summSquares() {
		
		int summ = 0;
	
		for(int i = 1; i<=100; i++) {
		summ += i*i;
		}
		
		System.out.println("summ: " + summ);
	}
	
//	â„–3.4	Ð¡Ð¾ÑÑ‚Ð°Ð²Ð¸Ñ‚ÑŒ Ð¿Ñ€Ð¾Ð³Ñ€Ð°Ð¼Ð¼Ñƒ Ð½Ð°Ñ…Ð¾Ð¶Ð´ÐµÐ½Ð¸Ñ Ð¿Ñ€Ð¾Ð¸Ð·Ð²ÐµÐ´ÐµÐ½Ð¸Ñ ÐºÐ²Ð°Ð´Ñ€Ð°Ñ‚Ð¾Ð² Ð¿ÐµÑ€Ð²Ñ‹Ñ… Ð´Ð²ÑƒÑ…ÑÐ¾Ñ‚ Ñ‡Ð¸ÑÐµÐ». 
	static void prodSquar() {
		
		double resProd = 1;
	     	int count = 0;
	     	double number = -5.0;

	     while (count <= 200) {
	    	 resProd *= number * number;
	         number += 0.01;
	         count++;
	     }
	     
	     System.out.println("ÐŸÑ€Ð¾Ð¸Ð·Ð²ÐµÐ´ÐµÐ½Ð¸Ðµ ÐºÐ²Ð°Ð´Ñ€Ð°Ñ‚Ð¾Ð² Ð¿ÐµÑ€Ð²Ñ‹Ñ… Ð´Ð²ÑƒÑ…ÑÐ¾Ñ‚ Ñ‡Ð¸ÑÐµÐ» Ñ€Ð°Ð²Ð½Ð¾ " + resProd);
	}
	
	
//	â„–3.7	 Ð”Ð»Ñ ÐºÐ°Ð¶Ð´Ð¾Ð³Ð¾ Ð½Ð°Ñ‚ÑƒÑ€Ð°Ð»ÑŒÐ½Ð¾Ð³Ð¾ Ñ‡Ð¸ÑÐ»Ð° Ð² Ð¿Ñ€Ð¾Ð¼ÐµÐ¶ÑƒÑ‚ÐºÐµ Ð¾Ñ‚ m Ð´Ð¾ n Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Ð²ÑÐµ Ð´ÐµÐ»Ð¸Ñ‚ÐµÐ»Ð¸,
//		 ÐºÑ€Ð¾Ð¼Ðµ ÐµÐ´Ð¸Ð½Ð¸Ñ†Ñ‹ Ð¸ ÑÐ°Ð¼Ð¾Ð³Ð¾ Ñ‡Ð¸ÑÐ»Ð°. m Ð¸ n Ð²Ð²Ð¾Ð´ÑÑ‚ÑÑ Ñ ÐºÐ»Ð°Ð²Ð¸Ð°Ñ‚ÑƒÑ€Ñ‹. 
 
	static void deliteli() {
		
		int m, n;
		
		Scanner scanNum = new Scanner(System.in);
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ m: ");
		m = scanNum.nextInt();
		
		System.out.println("Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ n: ");
		n = scanNum.nextInt();
		
		for(int i = m; i<=n; i++) {
			System.out.print("Ð§Ð¸ÑÐ»Ð¾: "+ i);
			
			for(int k = 2; k<i; k++) {
				
				if(i%k == 0) 				
				System.out.print(", Ð”ÐµÐ»Ð¸Ñ‚ÐµÐ»Ð¸: "+ k);
				
			}
			System.out.println();
		}
	
	}
